[run]
source =
    urllib3

omit =
    *urllib3/packages/*
    *urllib3/contrib/pyopenssl.py
    *urllib3/contrib/securetransport.py
    *urllib3/contrib/_securetransport/*

relative_files = True

[paths]
source =
    src/urllib3
    .nox/*/lib/python*/site-packages/urllib3
    .nox\*\Lib\site-packages\urllib3
    .nox/*/site-packages/urllib3

[report]
exclude_lines =
    except ImportError:
    pass
    import
    raise NotImplementedError
    .* # Platform-specific.*
    .*:.* # Python \d.*
    .* # Abstract
    .* # Defensive:
    if TYPE_CHECKING:
    ^\s*?\.\.\.\s*$
