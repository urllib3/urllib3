[run]
source =
    urllib3

omit =
    *urllib3/packages/*
    *urllib3/contrib/pyopenssl.py
    *urllib3/contrib/securetransport.py
    *urllib3/contrib/_securetransport/*

relative_files = True

[report]
exclude_lines =
    except ImportError:
    pass
    import
    raise NotImplementedError
    .* # Platform-specific.*
    .*:.* # Python \d.*
    .* # Abstract
    .* # Defensive:
    if TYPE_CHECKING:
    ^\s*?\.\.\.\s*$
